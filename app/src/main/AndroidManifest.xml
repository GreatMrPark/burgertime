<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.greateam.burgertime">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".game.example.UimakingByXmlActivity" />
        <activity android:name=".game.example.UiMakingByCodeActivity" />
        <activity android:name=".game.example.UimakingByCodeXmlActivity" />
        <activity android:name=".game.example.ConstrainLayoutActivity" />
        <activity android:name=".game.example.ViewEventActivity"></activity>
    </application>

</manifest>